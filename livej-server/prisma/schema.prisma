generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id    String @id @default(uuid())
  title String
  body  String
}

model User {
  id           String  @id @default(uuid())
  name         String
  email        String  @unique
  password     String
  refreshToken String?
}
